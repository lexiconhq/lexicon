  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/src/__mocks__/setup.js",
      "<rootDir>/src/__mocks__/setupLinking.ts"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/src/__mocks__/svgMock.js"
    }
  },
  "eslintConfig": {
    "extends": "kodefox/react-native",
    "plugins": [
      "t",
      "styles"
    ],
    "rules": {
      "t/string-literal": 1,
      "styles/style-maker-no-unused": 1,
      "operator-assignment": [
        "warn",
        "always"
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true,
  "name": "frontend",
  "version": "1.0.0"
}
