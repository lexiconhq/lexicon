    "react-native-svg-transformer": "^0.14.3",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "18.0.0",
    "runtypes": "^5.0.1",
    "ts-jest": "^26.1.4",
    "typescript": "^5.1.3",
    "zod": "^3.22.4"
