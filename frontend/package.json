{
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "quickstart": "expo -c",
    "typecheck": "tsc --noEmit -p .",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\"",
    "jestTest": "jest --coverage --passWithNoTests",
    "test": "yarn graphql:generate && yarn lint && yarn typecheck && yarn format:check && yarn jestTest",
    "eject": "expo eject",
    "graphql:generate": "graphql-codegen --config codegen.ts",
    "postinstall": "patch-package",
    "eas-build-post-install": "yarn --cwd ../ && yarn --cwd ../ generate",
    "android:push:encrypt": "openssl enc -aes-256-cbc -salt -md sha512 -pbkdf2 -in ./google-services.json -out ./google-services.json.enc -k",
    "dec-google-services": "openssl enc -aes-256-cbc -d -md sha512 -pbkdf2 -in ./google-services.json.enc -out ./google-services.json -k",
    "tests:android:build": "detox build -c android.emu.debug",
    "tests:android:test": "yarn graphql:generate && detox test -c android.emu.debug --headless --record-logs all --loglevel verbose --record-videos all --take-screenshots failing",
    "start:android:test": "DETOX_TESTS=true expo start --dev-client",
    "tests:ios:build": "yarn detox build --configuration ios.sim.debug",
    "tests:ios:pod:install": "cd ios && rm -rf insertyourappname.xcworkspace && rm -f Podfile.lock && pod install --repo-update && cd ..",
    "tests:ios:test": "yarn graphql:generate && detox test --configuration ios.sim.debug --cleanup --record-logs all --loglevel verbose --record-videos failing",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@expo-google-fonts/courier-prime": "^0.2.2",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-masked-view/masked-view": "0.3.1",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/elements": "^1.3.4",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/stack": "^6.2.2",
    "@shopify/flash-list": "1.6.4",
    "apollo-link-rest": "^0.9.0",
    "change-case": "^5.4.4",
    "date-fns": "^2.30.0",
    "expo": "~51.0.37",
    "expo-apple-authentication": "~6.4.2",
    "expo-constants": "~16.0.2",
    "expo-crypto": "^13.0.2",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-image-picker": "~15.0.7",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-network": "~6.0.1",
    "expo-notifications": "~0.28.18",
    "expo-screen-orientation": "~7.0.5",
    "expo-secure-store": "^13.0.2",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.27",
    "form-data": "^4.0.1",
    "graphql": "^16.9.0",
    "markdown-it-flowdock": "^0.3.8",
    "qs": "^6.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-native": "0.74.5",
    "react-native-animated-progress": "^1.0.2",
    "react-native-auto-grow-textinput": "^1.2.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-image-viewing": "^0.2.2",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-keyboard-accessory": "^0.1.16",
    "react-native-keyboard-aware-scroll-view": "0.9.5",
    "react-native-markdown-display": "^7.0.2",
    "react-native-modal-datetime-picker": "^17.1.0",
    "react-native-parsed-text": "^0.0.22",
    "react-native-pie-chart": "^3.0.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-rsa-native": "^2.0.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-svg": "15.2.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-webview": "13.8.6",
    "use-debounce": "^8.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@config-plugins/detox": "^8.0.0",
    "@graphql-codegen/add": "^5.0.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript": "4.0.7",
    "@graphql-codegen/typescript-operations": "^4.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@mswjs/http-middleware": "^0.10.2",
    "@types/apollo-upload-client": "^17.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^26.0.9",
    "@types/react": "~18.2.79",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^18.0.0",
    "cross-env": "^7.0.3",
    "detox": "^20.25.1",
    "eslint": "^8.56.0",
    "eslint-config-kodefox": "^2.0.0",
    "eslint-plugin-styles": "^1.0.0",
    "eslint-plugin-t": "^1.7.1",
    "express": "^4.20.0",
    "jest": "^29.2.1",
    "jest-circus": "^27.4.6",
    "jest-expo": "~51.0.4",
    "msw": "^2.6.8",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "react-native-console-time-polyfill": "^1.2.3",
    "react-native-svg-transformer": "^1.4.0",
    "react-native-testing-library": "^6.0.0",
    "react-test-renderer": "18.0.0",
    "reactotron-react-native": "^5.1.10",
    "runtypes": "^5.0.1",
    "ts-jest": "^26.1.4",
    "typescript": "^5.3.0",
    "zod": "^3.22.4"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/src/__mocks__/setup.js",
      "<rootDir>/src/__mocks__/setupLinking.ts"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/src/__mocks__/svgMock.js"
    }
  },
  "eslintConfig": {
    "extends": "kodefox/react-native",
    "plugins": [
      "t",
      "styles"
    ],
    "rules": {
      "t/string-literal": 1,
      "styles/style-maker-no-unused": 1,
      "operator-assignment": [
        "warn",
        "always"
      ]
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true,
  "name": "frontend",
  "version": "1.0.0"
}
